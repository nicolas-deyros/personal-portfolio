---
import List from '../List/List.astro'
import navData from '../../data/navData'
---

<ul class="lg:flex lg:gap-3">
	{
		navData.map(item => (
			<List
				href={item.path}
				icon={item.icon}
				text={item.text}
				title={item.text}
				data-navLink
			/>
		))
	}
</ul>

<script>
	import '../../js/nav'
</script>
